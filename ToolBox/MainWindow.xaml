        <TabControl>
            <TabItem Header="Android Device" Name="AndroidToolsTab"/>
            <TabItem Header="Quick Tools"   Name="QuickToolsTab"/>
            <TabItem Header="Log Viewer" Name="AppLogTab" />
        </TabControl>
    </Grid>
</Window>
